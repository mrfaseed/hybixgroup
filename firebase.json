{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "prerender": {
      "urls": [
        "/"
      ]
    }
  },
  "functions": {
    "source": "functions"
  }
}